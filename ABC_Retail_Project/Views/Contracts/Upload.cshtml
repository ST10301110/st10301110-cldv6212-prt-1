@model ABC_Retail_Project.Models.ContractUploadModel

<form asp-action="Upload" method="post" enctype="multipart/form-data" method="post">
    <div class="form-group">
        <label asp-for="CustomerId"></label>
        <input asp-for="CustomerId" class="form-control" />
    </div>

    <div class="form-group">
        <label>Contract File</label>
        <input type="file" name="contractFile" class="form-control" required />
    </div>

    <button type="submit" class="btn btn-primary">Upload</button>
</form>